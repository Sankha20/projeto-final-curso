	<queryString>
		<![CDATA[SELECT venda.id,venda.forma_pagamento,venda.total, cliente.nome, produto.album  FROM ((socasom.venda 
INNER JOIN
socasom.cliente  ON  cliente.id = venda.id_cliente
INNER JOIN 
socasom.item_venda ON item_venda.id_venda = venda.id)
INNER JOIN
socasom.produto ON item_venda.id_produto = produto.id);]]>
